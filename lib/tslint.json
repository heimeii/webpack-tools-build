{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "globals": {
        "require": true
    },
    "rules": {
        "space-before-function-paren": false,
        "no-angle-bracket-type-assertion": false,
        "callable-types": false,
        "unified-signatures": false,
        "no-string-throw": false,
        "ban-types": false,
        "no-console": [
            true,
            "debug",
            "info",
            "log",
            "time",
            "timeEnd",
            "trace"
        ],
        "no-consecutive-blank-lines": false
    }
}